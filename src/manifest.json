{
	"manifest_version": 2,
	"name": "oneshop-config-override",
	"description": "Override configurations coming from oneshop-configuration",
	"version": "1.2",
	"icons": {
		"48": "assets/images/icon-50.png",
		"128": "assets/images/icon-128.png"
	},
	"browser_action": {
		"default_title": "ONEShop-configuration-override"
	},
	"devtools_page": "./extensions/devtools.html",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"debugger",
		"storage",
		"tabs",
		"<all_urls>"
	]
}
