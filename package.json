		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"typescript": "~3.8.3 ",
		"webpack": "^5.38.1",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "~4.2.2"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,css}": [
			"prettier --write"
		]
	}
}
